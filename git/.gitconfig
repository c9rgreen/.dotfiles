[user]
	email = christopher.j.green@icloud.com
	name = Christopher Green
[alias]
	g = log --graph --abbrev-commit --date=iso --all
	t = tag --sort version:refname -n10 -n
	d = difftool
	s = status
	r = ls-remote
[github]
	user = cg433n
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[core]
	editor = nano
